{
    "tsserver.log": "normal",
    "coc.preferences.formatOnSaveFiletypes": [
      "javascript",
      "rust",
      "typescript",
      "typescriptreact",
      "javascriptreact",
      "css"
    ],
    "snippets.extends": {
      "typescriptreact": ["typescript", "javascript"]
